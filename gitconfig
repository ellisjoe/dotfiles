[color]
    ui = auto
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    ls = log --pretty=format:"%C(yellow)%h\\ %Cred%ad\\ %Cblue%cn\\ %Creset%s" --decorate
    ll = log --pretty=format:"%C(yellow)%h\\ %Cred%ad\\ %Cblue%cn\\ %Creset%s" --decorate --numstat
[push]
    default = simple
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    required = true
